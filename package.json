{
  "name": "cc-bot-lite",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_OPTIONS='--experimental-specifier-resolution=node' node --loader ts-node/esm/transpile-only --require tsconfig-paths/register src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/pagination": "^3.5.5",
    "axios": "^1.8.3",
    "discord.js": "^14.17.3",
    "discordx": "^11.12.2",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
