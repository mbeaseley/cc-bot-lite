{
  "name": "cc-bot-lite",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_OPTIONS='--experimental-specifier-resolution=node' node --loader ts-node/esm/transpile-only --require tsconfig-paths/register src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "@discordx/importer": "1.3.3",
    "@discordx/pagination": "4.4.0",
    "axios": "1.13.2",
    "discord.js": "14.25.1",
    "discordx": "11.13.3",
    "dotenv": "17.2.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.3",
    "eslint": "9.39.2",
    "nodemon": "3.1.11",
    "prettier": "3.7.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
